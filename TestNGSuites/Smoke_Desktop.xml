<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">


<suite thread-count="1" verbose="10" name="Germain Suite"
	annotations="JDK" >


	<listeners>
		<listener class-name="org.uncommons.reportng.HTMLReporter" />
		<listener class-name="org.uncommons.reportng.JUnitXMLReporter" />
	</listeners>

	<test name="Dashboards">
		<parameter name="browser-Type" value="chrome" />
		<parameter name="browserVersion" value="91" />
		<parameter name="osName" value="WINDOWS" />
		<parameter name="osVersion" value="8.1" />
		<parameter name="session" value="Testing" />
		<classes>
			<class name="com.testsuite.DashBoards.DashboardOverview"></class>
			<class name="com.testsuite.DashBoards.DashBoardTests"></class>
			<class name="com.testsuite.DashBoards.PortletsTests"></class>
			<class name="com.testsuite.DashBoards.PivotTests"></class>

		</classes>
	</test>

</suite>